@model ModuleBlockImage

<figure>
    <picture>
        <img src="@Model.Image.Url" alt="@Model.Image.AltText" height="@Model.Image.Height" width="@Model.Image.Width" style="object-fit: @Model.Settings.ObjectFit" />
    </picture>
    @if (Model.HasCaption)
    {
        <figcaption>
            @Html.Raw(Model.Caption)
        </figcaption>
    }
</figure>
